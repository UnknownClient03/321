        <!-- Femoral Pulses -->
        <LinearLayout
            android:id="@+id/layout_femoralpulses"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingBottom="8dp">

            <TextView
                android:id="@+id/label_femoralpulses"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Femoral Pulses"
                android:textStyle="bold"
                android:textSize="16sp"
                android:paddingBottom="4dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/label_isNormal_femoralpulses"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Is Normal"
                    android:gravity="center_vertical" />

                <CheckBox
                    android:id="@+id/checkBox_isNormal_femoralpulses"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/textInputLayout_comment_femoralpulses"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="Comment"
                android:paddingTop="4dp">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/editText_comment_femoralpulses"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="textMultiLine"
                    android:lines="2" />
            </com.google.android.material.textfield.TextInputLayout>
        </LinearLayout>

        <!-- Respiratory Rate -->
        <LinearLayout
            android:id="@+id/layout_respiratoryrate"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingBottom="8dp">

            <TextView
                android:id="@+id/label_respiratoryrate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Respiratory Rate"
                android:textStyle="bold"
                android:textSize="16sp"
                android:paddingBottom="4dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/label_isNormal_respiratoryrate"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Is Normal"
                    android:gravity="center_vertical" />

                <CheckBox
                    android:id="@+id/checkBox_isNormal_respiratoryrate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/textInputLayout_comment_respiratoryrate"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="Comment"
                android:paddingTop="4dp">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/editText_comment_respiratoryrate"
                    android:layout_width="match_parent"
